{
    "_config_name": "feeds.feeds_importer.documentation",
    "id": "documentation",
    "disabled": false,
    "config": {
        "name": "Documentation",
        "description": "Pulls docs files from b.org to docs.b.org",
        "fetcher": {
            "plugin_key": "FeedsHTTPFetcher",
            "config": {
                "auto_detect_feeds": false,
                "use_pubsubhubbub": false,
                "designated_hub": "",
                "request_timeout": null,
                "auto_scheme": "http",
                "accept_invalid_cert": false,
                "cache_http_result": true
            }
        },
        "parser": {
            "plugin_key": "FeedsJSONPathParser",
            "config": {
                "context": "$.nodes.*.node",
                "sources": {
                    "jsonpath_parser:0": "Nid",
                    "jsonpath_parser:1": "title",
                    "jsonpath_parser:2": "Callout",
                    "jsonpath_parser:3": "'Documentation Level'",
                    "jsonpath_parser:4": "'Documentation Status'",
                    "jsonpath_parser:5": "Body",
                    "jsonpath_parser:6": "'Original source title'",
                    "jsonpath_parser:7": "'Original source url'",
                    "jsonpath_parser:8": "Username"
                },
                "debug": {
                    "options": {
                        "context": 0,
                        "jsonpath_parser:0": 0,
                        "jsonpath_parser:1": 0,
                        "jsonpath_parser:2": 0,
                        "jsonpath_parser:3": 0,
                        "jsonpath_parser:4": 0,
                        "jsonpath_parser:5": 0,
                        "jsonpath_parser:6": 0,
                        "jsonpath_parser:7": 0,
                        "jsonpath_parser:8": 0
                    }
                },
                "allow_override": 0,
                "convert_four_byte": 0
            }
        },
        "processor": {
            "plugin_key": "FeedsNodeProcessor",
            "config": {
                "expire": "-1",
                "author": "38",
                "authorize": 0,
                "mappings": [
                    {
                        "source": "jsonpath_parser:0",
                        "target": "field_lecacy_nid",
                        "unique": 1,
                        "language": "und"
                    },
                    {
                        "source": "jsonpath_parser:1",
                        "target": "title",
                        "unique": 0
                    },
                    {
                        "source": "jsonpath_parser:2",
                        "target": "field_callout",
                        "unique": false,
                        "language": "und"
                    },
                    {
                        "source": "jsonpath_parser:3",
                        "target": "field_docs_level",
                        "unique": false,
                        "language": "und"
                    },
                    {
                        "source": "jsonpath_parser:4",
                        "target": "field_docs_status",
                        "unique": false,
                        "language": "und"
                    },
                    {
                        "source": "jsonpath_parser:5",
                        "target": "body",
                        "unique": false,
                        "language": "und"
                    },
                    {
                        "source": "jsonpath_parser:6",
                        "target": "field_docs_source:title",
                        "unique": false,
                        "language": "und"
                    },
                    {
                        "source": "jsonpath_parser:7",
                        "target": "field_docs_source:url",
                        "unique": false,
                        "language": "und"
                    },
                    {
                        "source": "jsonpath_parser:8",
                        "target": "user_name",
                        "unique": false,
                        "language": "und"
                    }
                ],
                "insert_new": "1",
                "update_existing": "2",
                "update_non_existent": "skip",
                "input_format": "documentation",
                "skip_hash_check": 0,
                "bundle": "docs",
                "language": "und"
            }
        },
        "content_type": "",
        "update": 0,
        "import_period": "43200",
        "expire_period": 3600,
        "import_on_create": 1,
        "process_in_background": 0
    }
}
